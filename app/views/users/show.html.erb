<div class="view">
 <div class="nav">
  <ul>
   <li><%= link_to "アカウント", user_path %></li>
   <li><%= link_to "プロフィール", edit_user_path %></li>
  </ul>
 </div>
 <div class="form-wrapper color-form-wrapper">
  <div class= "form">
   <%= form_with model: @user do |f| %>
   <div class="row">
    <h5>アカウント</h5>
   </div>
    <div class="row">
      <ul>
       <li><%= f.label :email, "メールアドレス" %></li>
       <li><%= f.file_field :email %></li>
      </ul>
    </div>
    <div class="row">
      <ul>
       <li><%= "パスワード" %></li>
       <li>********</li>
      </ul>
    </div>
    <button><%= link_to "編集", edit_user_registration_path %></button>
   </div>
   <% end %>
  </div>
 </div>
</div>
